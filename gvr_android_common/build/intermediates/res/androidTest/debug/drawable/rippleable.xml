<?xml version="1.0" encoding="utf-8"?>
<!--
  Background that displays a white transparent color when touched.
  This does no actual rippling pre-21, but is overridden on 21 to use a RippleDrawable.
-->
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:drawable="@color/white_transparent"
        android:state_pressed="true"/>
    <item android:drawable="@android:color/transparent"/>
</selector>
